{
  "redirects": [
    {
      "source": "/note/:noteId",
      "destination": "/note?id=:noteId",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/.well-known/:path*",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
